{
  "name": "reviewpackets",
  "version": "1.0.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "build:frontend": "cd frontend && npm install && npm run build",
    "build:backend": "powershell -ExecutionPolicy Bypass -File scripts/build-backend.ps1",
